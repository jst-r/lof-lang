class Test {}

var a = Test();
a.b = 1;
assert(a.b == 1, "");

a.b = a.b + 1;
assert(a.b == 2, "");

// a fake method

fn f() {
    return "foo";
}
a.f = f;
print(a.f());
assert(a.f() == "foo", "");

// Methods
class HasMethods {
    hi() {
        return "hi!";
    }

    hasThis() {
        return this.a;
    }
}

var b = HasMethods();

assert(b.hi() == "hi!", "");

b.a = 1;
assert(b.hasThis() == 1, "");