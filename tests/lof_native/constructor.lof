class Test {
    init() {
        this.a = 1;
    }
}

var instance = Test();
assert(instance.a == 1, "");

class Counter {
    init(step) {
        this.count = 0;
        this.step = step;
    }

    next() {
        this.count = this.count + this.step;
        return this.count;
    }
}

var counter = Counter(1);
assert(counter.next() == 1, "");
assert(counter.next() == 2, "");
assert(counter.next() == 3, "");
assert(counter.next() == 4, "");